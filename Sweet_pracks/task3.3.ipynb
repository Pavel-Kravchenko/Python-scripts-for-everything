    "\n",
    "def lcm(a, b):\n",
    "    m = a * b\n",
    "    while a != 0 and b != 0:\n",
    "        if a > b:\n",
    "            a %= b\n",
    "        else:\n",
    "            b %= a\n",
    "    return m // (a + b)\n",
    "\n",
    "\n",
    "input_l = input().strip().split()\n",
    "one = int(input_l[0])\n",
    "two = int(input_l[1])\n",
    "print(lcm(one, two))"
   ]
  }
