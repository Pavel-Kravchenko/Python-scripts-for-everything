    "\n",
    "\n",
    "def check2rec(num):\n",
    "    if num == 1:\n",
    "        return \"YES\"\n",
    "    if num & 1:\n",
    "        return \"NO\"\n",
    "    return check2rec(num >> 1)\n",
    "\n",
    "\n",
    "out_ = check2rec(input_)\n",
    "print(out_)"
